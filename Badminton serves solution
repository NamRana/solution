#include<bits/stdc++.h>
using namespace std;
#define ln "\n"

void solve(){
   int n;
    cin>>n;
    if(n%2==0){cout<<((n/2)+1)<<ln;}
    else{
        cout<<(n+1)/2<<ln;
    }
}

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);cout.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    solve();  
}
